{
    "name": "zehntech/readability",
    "description": "A laravel port of Readability.js",
    "keywords": ["laravel readability", "html", "PHP readability in laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Omkar Kurmi",
            "email": "omkar.kurmi@zehntech.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-mbstring": "*",
        "masterminds/html5": "^2.8",
        "league/uri": "~6.7.2"
    },
    "require-dev": {
        "masterminds/html5": "^2.8",
        "league/uri": "~6.7.2"
    },
    "autoload": {
        "psr-4": {
            "Zehntech\\Readability\\": "src/",
            "Masterminds\\": "masterminds/html5/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zehntech\\Readability\\ReadabilityServiceProvider"
            ]
        }
    }
}
